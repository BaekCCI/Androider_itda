<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    xmlns:app="http://schemas.android.com/apk/res-auto">


    <View
        android:id="@+id/noticeLogoutCheck"
        android:layout_width="320dp"
        android:layout_height="170dp"
        android:background="@drawable/noticewindow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:background="@drawable/view_round_white_6"
        android:id="@+id/noticeLogoutCheckMsg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"

        android:text="완료되었습니다."
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="@id/noticeLogoutCheck"
        app:layout_constraintStart_toStartOf="@+id/noticeLogoutCheck"
        app:layout_constraintTop_toTopOf="@+id/noticeLogoutCheck" />

    <Button
        android:id="@+id/okBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:background="@drawable/btnshape_green"
        android:text="확인"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@id/noticeLogoutCheck"
        app:layout_constraintEnd_toEndOf="@id/noticeLogoutCheck"
        app:layout_constraintStart_toStartOf="@id/noticeLogoutCheck" />
</androidx.constraintlayout.widget.ConstraintLayout>